<li>
<%= form_for(:session, html: { class: "loginForm form-inline" }, url: login_path) do |f| %>
<div class="form-group">
  <%= f.label :email %>
  <%= f.email_field :email, class: "form-control", id: "email", placeholder: "Enter email" %>
</div>
<div class="form-group">
  <%= f.label :password %>
  <%= f.password_field :password, class: "form-control", id: "pwd", placeholder: "Enter password" %>
</div>
  <%= f.submit "Log in", class: "login"  %>
<% end %>
</li>